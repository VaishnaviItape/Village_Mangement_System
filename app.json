{
  "expo": {
    "name": "village-management-system",
    "slug": "village-management-system",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "villagemanagementsystem",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "developmentClient": {
      "silentLaunch": true
    },

    "ios": {
      "bundleIdentifier": "com.villagemanagementsystem",
      "buildNumber": "1.0.0",
      "supportsTablet": false
    },

    "android": {
      "package": "com.villagemanagementsystem",
      "versionCode": 1,

      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            { "scheme": "https", "host": "villagemanagementsystem.in" },
            { "scheme": "villagemanagementsystem" }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],

      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },

      "config": {
        "googleMaps": {
          "apiKey": "YOUR GOOGLE MAP API KEY HERE"
        }
      },

      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ]
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "apiUrl": "https://village-management-api.in",
      "router": {},
      "eas": {
        "projectId": "b97b34ce-731a-44c0-a06d-9ef17441fd04"
      }
    }
  }
}
